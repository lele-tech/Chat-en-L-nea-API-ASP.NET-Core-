            "format": "double"
          },
          "estado": {
            "type": "string"
          },
          "fechaRegistro": {
            "type": "string",
            "format": "date-time"
          },
          "foto": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Message": {
