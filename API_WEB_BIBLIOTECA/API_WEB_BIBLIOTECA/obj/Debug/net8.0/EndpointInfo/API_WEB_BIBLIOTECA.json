        "type": "object",
        "properties": {
          "isbn": {
            "type": "integer",
            "format": "int32"
          },
          "titulo": {
            "maxLength": 150,
            "minLength": 10,
            "type": "string"
          },
          "editorial": {
            "maxLength": 150,
            "minLength": 10,
            "type": "string"
          },
          "annio": {
