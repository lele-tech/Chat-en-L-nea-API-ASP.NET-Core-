            "type": "integer",
            "format": "int32"
          },
          "groupName": {
            "type": "string",
            "nullable": true
          },
          "userName": {
            "type": "string",
            "nullable": true
          },
          "mensaje": {
            "type": "string",
            "nullable": true
          },
          "timeSent": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "Usuario": {
        "required": [
          "email",
          "estado",
          "fechaRegistro",
          "nombreCompleto",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "nombreCompleto": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "format": "password"
          },
          "fechaRegistro": {
            "type": "string",
            "format": "date-time"
          },
          "estado": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "$ref": "#/components/schemas/DateOnly"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
